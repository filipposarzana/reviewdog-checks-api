{
  "rules": {
    "semicolon": [true, "always"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "never"
        },
        "esSpecCompliant": true
      }
    ]
  }
}
